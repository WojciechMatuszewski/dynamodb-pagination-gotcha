  },
  "devDependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "2.89.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "2.89.0-alpha.0",
    "@types/aws-lambda": "8.10.119",
    "@types/node": "20.4.5",
    "aws-cdk": "2.89.0",
    "esbuild": "0.18.17"
  }
